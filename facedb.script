INSERT INTO SOURCES VALUES(15,'Fuel prices','Substance','if(v.name != &quot;Electricity&quot; &amp;&amp; v.name != &quot;CO2&quot;){\u000a      price = v.in(&quot;TRADED_SUBSTANCE&quot;).in(&quot;MARKET_POINT&quot;).filter{it.time == tick}.collect{it.price};\u000a      density = v.energyDensity;\u000a      inGJ = price[0] / density;\u000a      return [v.name,inGJ];\u000a}')
